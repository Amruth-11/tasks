    <script>pipeline {
    agent any
    environment {
        DOCKER_CREDENTIALS_ID = &apos;amruth11&apos; // Replace with your Docker Hub credentials ID
        DOCKER_IMAGE = &apos;amruth11/new_webproject:latest&apos; // Change to your Docker Hub repository
        CONTAINER_NAME = &apos;MyContainer&apos; // Name of the container to manage
    }
    stages {
        stage(&apos;Clone Repository&apos;) {
            steps {
                git &apos;https://github.com/Amruth-11/new_webproject.git&apos; // Replace with your repo URL
            }
        }
        stage(&apos;Build Docker Image&apos;) {
            steps {
                script {
                    docker.build(DOCKER_IMAGE)
                }
            }
        }
        stage(&apos;Push Docker Image&apos;) {
            steps {
                script {
                    docker.withRegistry(&apos;https://index.docker.io/v1/&apos;, DOCKER_CREDENTIALS_ID) {
                        docker.image(DOCKER_IMAGE).push()
                    }
                }
            }
        }
        stage(&apos;Deploy&apos;) {
            steps {
                script {
                    // Check if the container exists and delete it if it does
                    def existingContainer = sh(script: &quot;docker ps -q -f name=${CONTAINER_NAME}&quot;, returnStdout: true).trim()
                    if (existingContainer) {
                        bat &quot;docker stop ${CONTAINER_NAME} || echo Container not found&quot;
                        bat &quot;docker rm ${CONTAINER_NAME} || echo Container not found&quot;
                    }
                    // Run the new container
                    bat &quot;docker run -d --name ${CONTAINER_NAME} -p 8024:80 ${DOCKER_IMAGE}&quot;
                }
            }
        }
    }

    post {
        always {
            // Clean up
            cleanWs()
        }
    }
}
</script>
