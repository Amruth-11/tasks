    <script>pipeline {
    agent any

    environment {
        DOCKER_IMAGE = &apos;amruth11/ci-cd-pipeline:latest&apos;
        DOCKER_CREDENTIALS_ID = &apos;amruth11&apos;
        GIT_REPO = &apos;https://github.com/Amruth-11/ci-cd-pipeline.git&apos;
    }

    stages {
        stage(&apos;Checkout&apos;) {
            steps {
                git branch: &apos;master&apos;, url: &quot;${GIT_REPO}&quot;
            }
        }



        stage(&apos;Docker Build&apos;) {
            steps {
                script {
                    // Build Docker image
                    sh &quot;docker build -t ${DOCKER_IMAGE} .&quot;
                }
            }
        }

        stage(&apos;Docker Login&apos;) {
            steps {
                script {
                    // Login to Docker Hub
                    docker.withRegistry(&apos;https://index.docker.io/v1/&apos;, &quot;${DOCKER_CREDENTIALS_ID}&quot;) {
                        // Push the Docker image
                        sh &quot;docker push ${DOCKER_IMAGE}&quot;
                    }
                }
            }
        }

        stage(&apos;Deploy&apos;) {
            steps {
                script {
                    // Optional: Stop existing container if it exists
                    sh &quot;docker stop ci-cd-pipeline || true&quot;
                    sh &quot;docker rm sci-cd-pipeline || true&quot;

                    // Deploy the new container
                    sh &quot;docker run -d --name ci-cd-pipeline -p 8086:8086 ${DOCKER_IMAGE}&quot;
                }
            }
        }
    }

    post {
        success {
            echo &apos;Pipeline completed successfully!&apos;
        }
        failure {
            echo &apos;Pipeline failed!&apos;
        }
    }
}
</script>
